{
    "class": "DO",
    "declaration": {
        "schemaVersion": "1.5.0",
        "class": "Device",
        "async": true,
        "Common": {
            "class": "Tenant",
            "hostname": "{{bip1_hostname}}.{{domain}}",
            "myLicense": {
                "class": "License",
                "licenseType": "licensePool",
                "unitOfMeasure": "yearly",
                "licensePool": "F5-BIG-MSP-LOADV04-LIC",
                "skuKeyword1": "F5-BIG-MSP-LTM-1G",
                "bigIpUsername": "{{user}}",
                "bigIpPassword": "{{temp_password}}"
            },
            "myProvision": {
                "class": "Provision",
                "ltm": "nominal",
                "avr": "nominal"
            },
            "myDbVariables": {
                "class": "DbVariables",
                "ui.advisory.enabled": "true",
                "ui.advisory.color": "red",
                "ui.advisory.text": "{{bip1_hostname}}.{{domain}}"
            },
            "myDns": {
                "class": "DNS",
                "nameServers": [
                    "10.0.0.2"
                ],
                "search": [
                    "{{domain}}"
                ]
            },
            "myNtp": {
                "class": "NTP",
                "servers": [
                    "pool.ntp.org"
                ],
                "timezone": "America/Chicago"
            },
            "ext_vlan": {
                "class": "VLAN",
                "interfaces": [
                    {
                        "name": "1.1",
                        "tagged": false
                    }
                ]
            },
            "int_vlan": {
                "class": "VLAN",
                "interfaces": [
                    {
                        "name": "1.2",
                        "tagged": false
                    }
                ]
            }
        }
    },
    "targetUsername": "{{user}}",
    "targetHost": "{{bip1_mgmt_address}}",
    "targetPassphrase": "{{temp_password}}",
    "bigIqSettings": {
        "failImportOnConflict": false,
        "conflictPolicy": "USE_BIGIQ",
        "deviceConflictPolicy": "USE_BIGIP",
        "versionedConflictPolicy": "KEEP_VERSION"
    }
}