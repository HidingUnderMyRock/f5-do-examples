{
    "class": "DO",
    "declaration": {
        "schemaVersion": "1.5.0",
        "class": "Device",
        "async": true,
        "Common": {
            "class": "Tenant",
            "hostname": "{{bip5_hostname}}.{{domain}}",
            "myLicense": {
                "class": "License",
                "licenseType": "licensePool",
                "unitOfMeasure": "yearly",
                "licensePool": "F5-BIG-MSP-LOADV04-LIC",
                "skuKeyword1": "F5-BIG-MSP-DNSVE-1M",
                "bigIpUsername": "{{user}}",
                "bigIpPassword": "{{password}}"
            },
            "myProvision": {
                "class": "Provision",
                "ltm": "nominal",
                "gtm": "nominal",
                "avr": "nominal"
            },
            "myDbVariables": {
                "class": "DbVariables",
                "ui.advisory.enabled": "true",
                "ui.advisory.color": "red",
                "ui.advisory.text": "{{bip5_hostname}}.{{domain}}"
            },
            "myDns": {
                "class": "DNS",
                "nameServers": [
                    "10.0.0.2"
                ],
                "search": [
                    "{{domain}}"
                ]
            },
            "myNtp": {
                "class": "NTP",
                "servers": [
                    "pool.ntp.org"
                ],
                "timezone": "America/Chicago"
            },
            "ext_vlan": {
                "class": "VLAN",
                "interfaces": [
                    {
                        "name": "1.1",
                        "tagged": false
                    }
                ]
            },
            "int_vlan": {
                "class": "VLAN",
                "interfaces": [
                    {
                        "name": "1.2",
                        "tagged": false
                    }
                ]
            },
            "ext_self": {
                "class": "SelfIp",
                "address": "{{bip5_ext_address}}/24",
                "vlan": "ext_vlan"
            },
            "int_self": {
                "class": "SelfIp",
                "address": "{{bip5_int_address}}/24",
                "vlan": "int_vlan"
            },
            "defaultRoute": {
                "class": "Route",
                "gw": "{{east_default_route}}",
                "network": "default"
            },
            "admin": {
                "class": "User",
                "userType": "regular",
                "password": "{{password}}",
                "shell": "tmsh",
                "keys": [
                    "ssh-rsa  lipowsky-aws"
                ]
            }
        }
    },
    "targetUsername": "{{user}}",
    "targetHost": "{{bip5_mgmt_address}}",
    "targetPassphrase": "{{temp_password}}",
    "bigIqSettings": {
        "failImportOnConflict": false,
        "conflictPolicy": "USE_BIGIQ",
        "deviceConflictPolicy": "USE_BIGIP",
        "versionedConflictPolicy": "KEEP_VERSION",
        "statsConfig": {
            "enabled": true,
            "zone": "{{east_zone}}"
        }
    }
}